{% extends 'layout.html.twig' %}
{% set active_section = 'file_upload' %}
{% form_theme form '@LeaptCore/Form/form_layout.html.twig' %}

{% block body %}
    <h1>Update a gallery</h1>

    {{ form_start(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.localImageFile) }}
        {{ form_row(form.s3ImageFile) }}
        {{ form_row(form.s3AsyncImageFile) }}

        <p><button type="submit">Update gallery</button></p>
    {{ form_end(form) }}
{% endblock body %}
