{% extends 'layout.html.twig' %}
{% set active_section = 'file_upload_collection' %}
{% if datalist is defined %}
    {% paginator_theme datalist.paginator '@LeaptCore/Paginator/paginator_bootstrap5_layout.html.twig' %}
{% endif %}
{% if form is defined %}
    {% form_theme form with [
        'bootstrap_5_layout.html.twig',
        '@LeaptCore/Form/bootstrap_5_layout.html.twig',
        'album/form_layout.html.twig',
    ] %}
{% endif %}

{% block stylesheets %}
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
{% endblock stylesheets %}

{% block javascripts %}
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
{% endblock javascripts %}
